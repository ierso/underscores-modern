{
  "name": "automatic",
  "version": "1.0.0",
  "description": "Underscores using a modern build.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run sass:watch\" \"npm run dev\"",
    "build": "concurrently \"npm run sass:compile\" \"npm run production\"",
    "dev": "webpack --watch --mode=development",
    "production": "webpack --watch --mode=production",
    "sass:watch": "nodemon -e scss -x \"npm run sass:compile\"",
    "sass:compile": "node-sass src/styles -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "homepage": "https://github.com/Automattic/_s#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-loader": "^8.0.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "prettier": "1.15.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@webpack-cli/init": "^0.1.2",
    "axios": "^0.18.0",
    "browser-sync": "^2.26.3",
    "concurrently": "^4.0.1",
    "css-loader": "^2.1.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "replace-in-file": "^3.4.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.0.1"
  }
}
